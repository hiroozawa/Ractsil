<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/navigation_list">
    <fragment
        android:id="@+id/navigation_list"
        android:name="com.hiroozawa.ractsil.ui.list.CarListFragment"
        android:label="List"
        tools:layout="@layout/fragment_car_list" >
        <action
            android:id="@+id/action_navigation_list_to_navigation_map"
            app:destination="@id/navigation_map" />
    </fragment>
    <fragment
        android:id="@+id/navigation_map"
        android:name="com.hiroozawa.ractsil.ui.map.MapFragment"
        android:label="Map"
        tools:layout="@layout/fragment_map" >
        <action
            android:id="@+id/action_navigation_map_to_navigation_list"
            app:destination="@id/navigation_list" />
    </fragment>
</navigation>